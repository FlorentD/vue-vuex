{
  "name": "vuejs-vuex",
  "version": "1.0.0",
  "description": "All I need to start my next vuejs/vuex app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "predist": "rm -fr dist && mkdir dist && mkdir dist/js",
    "prettier": "prettier --single-quote --trailing-comma es5 --write",
    "dist": "webpack --config webpack.prod.config.js --progress --colors && npm run dist:html",
    "dist:html": "htmlprocessor ./index.html -o dist/index.html",
    "test": "jest --config jest.config.json"
  },
  "keywords": [
    "vuejs",
    "vuex"
  ],
  "author": "Florent DUVEAU",
  "license": "ISC",
  "dependencies": {
    "vue": "2.5.17",
    "vue-form": "4.9.0",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.24",
    "babel-core": "6.26.3",
    "babel-env": "2.4.1",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "css-loader": "1.0.0",
    "htmlprocessor": "0.2.6",
    "jest": "23.4.2",
    "mini-css-extract-plugin": "0.4.1",
    "prettier": "1.14.2",
    "restify": "7.2.1",
    "style-loader": "0.22.1",
    "vue-jest": "2.6.0",
    "vue-loader": "15.3.0",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-livereload-plugin": "2.1.1"
  }
}
